<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="jun.dao.TStaffDao">

    <resultMap type="jun.entity.TStaff" id="TStaffMap">
        <result property="sId" column="s_id" jdbcType="INTEGER"/>
        <result property="sName" column="s_name" jdbcType="VARCHAR"/>
        <result property="sSex" column="s_sex" jdbcType="VARCHAR"/>
        <result property="sPhonenumber" column="s_phonenumber" jdbcType="VARCHAR"/>
        <result property="sEmal" column="s_emal" jdbcType="VARCHAR"/>
        <result property="sEducation" column="s_education" jdbcType="VARCHAR"/>
        <result property="sIdno" column="s_idno" jdbcType="VARCHAR"/>
        <result property="sAddress" column="s_address" jdbcType="VARCHAR"/>
        <result property="sTime" column="s_time" jdbcType="TIMESTAMP"/>
        <result property="sPid" column="s_pid" jdbcType="INTEGER"/>
        <result property="sDid" column="s_did" jdbcType="INTEGER"/>
    </resultMap>

    <!--查询单个-->
    <select id="queryById" resultMap="TStaffMap">
        select
          s_id, s_name, s_sex, s_phonenumber, s_emal, s_education, s_idno, s_address, s_time, s_pid, s_did
        from db_personnelmanagement.t_staff
        where s_id = #{sId}
    </select>

    <!--查询指定行数据-->
    <select id="queryAllByLimit" resultMap="TStaffMap">
        select
          s_id, s_name, s_sex, s_phonenumber, s_emal, s_education, s_idno, s_address, s_time, s_pid, s_did
        from db_personnelmanagement.t_staff
        limit #{offset}, #{limit}
    </select>

    <!--通过实体作为筛选条件查询-->
    <select id="queryAll" resultMap="TStaffMap">
        select
          s_id, s_name, s_sex, s_phonenumber, s_emal, s_education, s_idno, s_address, s_time, s_pid, s_did
        from db_personnelmanagement.t_staff
        <where>
            <if test="sId != null">
                and s_id = #{sId}
            </if>
            <if test="sName != null and sName != ''">
                and s_name = #{sName}
            </if>
            <if test="sSex != null and sSex != ''">
                and s_sex = #{sSex}
            </if>
            <if test="sPhonenumber != null and sPhonenumber != ''">
                and s_phonenumber = #{sPhonenumber}
            </if>
            <if test="sEmal != null and sEmal != ''">
                and s_emal = #{sEmal}
            </if>
            <if test="sEducation != null and sEducation != ''">
                and s_education = #{sEducation}
            </if>
            <if test="sIdno != null and sIdno != ''">
                and s_idno = #{sIdno}
            </if>
            <if test="sAddress != null and sAddress != ''">
                and s_address = #{sAddress}
            </if>
            <if test="sTime != null">
                and s_time = #{sTime}
            </if>
            <if test="sPid != null">
                and s_pid = #{sPid}
            </if>
            <if test="sDid != null">
                and s_did = #{sDid}
            </if>
        </where>
    </select>

    <!--新增所有列-->
    <insert id="insert" keyProperty="sId" useGeneratedKeys="true">
        insert into db_personnelmanagement.t_staff(s_name, s_sex, s_phonenumber, s_emal, s_education, s_idno, s_address, s_time, s_pid, s_did)
        values (#{sName}, #{sSex}, #{sPhonenumber}, #{sEmal}, #{sEducation}, #{sIdno}, #{sAddress}, #{sTime}, #{sPid}, #{sDid})
    </insert>

    <!--通过主键修改数据-->
    <update id="update">
        update db_personnelmanagement.t_staff
        <set>
            <if test="sName != null and sName != ''">
                s_name = #{sName},
            </if>
            <if test="sSex != null and sSex != ''">
                s_sex = #{sSex},
            </if>
            <if test="sPhonenumber != null and sPhonenumber != ''">
                s_phonenumber = #{sPhonenumber},
            </if>
            <if test="sEmal != null and sEmal != ''">
                s_emal = #{sEmal},
            </if>
            <if test="sEducation != null and sEducation != ''">
                s_education = #{sEducation},
            </if>
            <if test="sIdno != null and sIdno != ''">
                s_idno = #{sIdno},
            </if>
            <if test="sAddress != null and sAddress != ''">
                s_address = #{sAddress},
            </if>
            <if test="sTime != null">
                s_time = #{sTime},
            </if>
            <if test="sPid != null">
                s_pid = #{sPid},
            </if>
            <if test="sDid != null">
                s_did = #{sDid},
            </if>
        </set>
        where s_id = #{sId}
    </update>

    <!--通过主键删除-->
    <delete id="deleteById">
        delete from db_personnelmanagement.t_staff where s_id = #{sId}
    </delete>

</mapper>